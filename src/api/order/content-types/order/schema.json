{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "shippingMethod": {
      "type": "enumeration",
      "enum": [
        "normal",
        "express",
        "onSite"
      ]
    },
    "standing": {
      "type": "enumeration",
      "default": "unpaid",
      "enum": [
        "unpaid",
        "paid",
        "shipped",
        "arrived",
        "completed"
      ]
    },
    "address": {
      "type": "component",
      "component": "address.address",
      "repeatable": false
    },
    "totalPrice": {
      "type": "decimal",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "stripeId": {
      "type": "string",
      "unique": true
    },
    "trackingCode": {
      "type": "string"
    }
  }
}
